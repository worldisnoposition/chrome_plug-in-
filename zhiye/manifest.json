{
"manifest_version":2,
"name":"WORK_SPYDER_MAN",
"description":"the plugin can crawl work information from website automaticly",
"version":"1.0",
"permissions":[
    "https://sou.zhaopin.com/*"
    ],
"browser_action":{
    "default_popup":"my.html"
    },
	"content_scripts": [
		{
		  "matches": ["https://sou.zhaopin.com/*","http://sou.zhaopin.com/*"],
		  "js": ["jquery.1.8.js","zhilian.js"]
		},
		{
		  "matches": ["https://jobs.zhaopin.com/*","http://jobs.zhaopin.com/*"],
		  "js": ["jquery.1.8.js","jobs.js"]
		},
		{
		  "matches": ["https://www.zhipin.com/*"],
		  "js": ["jquery.1.8.js","boss.js"]
		},
		{
		  "matches": ["https://www.zhipin.com/job_detail/*"],
		  "js": ["jquery.1.8.js","bossdetail.js"]
		},
		{
		  "matches": ["https://cn.100offer.com/job_positions"],
		  "js": ["jquery.1.8.js","100offer.js"]
		}
	]
}